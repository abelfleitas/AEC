<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="FillableLoader">
    <attr name="fl_fillColor"
        format="color"/>
    <attr name="fl_strokeColor"
        format="color"/>
    <attr name="fl_strokeWidth"
        format="dimension"/>
    <attr name="fl_originalWidth"
        format="integer"/>
    <attr name="fl_originalHeight"
        format="integer"/>
    <attr name="fl_strokeDrawingDuration"
        format="integer"/>
    <attr name="fl_fillDuration"
        format="integer"/>
    <attr name="fl_fillPercentage"
        format="integer"/>
    <attr name="fl_clippingTransform"
        format="enum">
      <enum name="plain"
          value="0"/>
      <enum name="spikes"
          value="1"/>
      <enum name="rounded"
          value="2"/>
      <enum name="waves"
          value="3"/>
      <enum name="squares"
          value="4"/>
      <enum name="bites"
          value="5"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="ArcDrawer">
    <attr name="arc_width" format="dimension"/>
    <attr name="arc_cropDirection" format="enum">
      <enum name="cropInside" value="0"/>
      <enum name="cropOutside" value="1"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="ButtonBarContainerTheme">
    <attr name="metaButtonBarStyle" format="reference" />
    <attr name="metaButtonBarButtonStyle" format="reference" />
  </declare-styleable>

</resources>